{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "eslint.validate": [
    "javascript",
    "typecript",
    "vue"
  ],
  "css.lint.unknownAtRules": "ignore",
  "tailwindCSS.includeLanguages": {
    "vue": "html",
    "vue-html": "html"
  },
  "vue3snippets.enable-compile-vue-file-on-did-save-code": true
}
