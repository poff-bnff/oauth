{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "eslint.validate": [
    "javascript",
    "typecript",
    "vue"
  ],
  "css.lint.unknownAtRules": "ignore",
  "tailwindCSS.includeLanguages": {
    "vue": "html",
    "vue-html": "html"
  }
}
